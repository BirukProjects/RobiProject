            <div id="company-inner-container" class="company-inner-container container-24">

                <div id="company-tabs-page" class="company-tabs-content">




                    <div data-ng-controller="CompanyController">

                        <div class="company-content-container container-22">

                            <div class="our-directory block">
                                <form class="subscription-table" name="addCompany" data-ng-show="addMode">
                                    <div id="subscription-options">
                                        <div class="subscription-column">
                                            <div class="subscription-header"> <span class="text-colorful">Company Address</span></div>
                                            <div class="subscription-body">
                                                <br />
                                                <input type="text" class="text-input-grey" placeholder="Company Name" data-ng-model="newCompany.companyName" required />
                                                <br />
                                                <input type="text" class="text-input-grey datepicker" placeholder="Founded Date" data-ng-model="newCompany.CompanyFoundedDate" required />
                                                <br />
                                                <input type="text" class="text-input-grey" placeholder="Company Website" data-ng-model="newCompany.CompanyWebsite" required />
                                                <br />

                                                <input type="text" class="text-input-grey" placeholder="Company Email" data-ng-model="newCompany.CompanyEmail1" required />
                                                <br />

                                                <input type="text" class="text-input-grey" placeholder="Company Email 2" data-ng-model="newCompany.CompanyEmail2" required />


                                            </div>
                                        </div>
                                        <div class="subscription-column">
                                            <div class="subscription-header"> <span class="text-colorful">Basic</span></div>
                                            <div class="subscription-body">
                                                <br />

                                                <select data-ng-model="newCompany.CompanyTypeId" class="select-input-grey" data-ng-options="c.companyTypeId as c.companyTypeName for c in CompanyTypeList">
                                                    <option value="">-- Select CompanyType --</option>
                                                </select>
                                                <br />

                                                <select data-ng-model="newCompany.UserId" class="select-input-grey" data-ng-options="u.id as u.email for u in userList">
                                                    <option value="">-- Select Owner --</option>
                                                </select>
                                                <br />

                                                <input type="tel" class="text-input-grey" placeholder="Telephone" data-ng-model="newCompany.CompanyTelephone" required />
                                                <br />

                                                <input type="text" class="text-input-grey" placeholder="number Of Employees" data-ng-model="newCompany.numberOfEmployees" required />
                                                <br />

                                                <input type="number" class="text-input-grey" placeholder="turnover" data-ng-model="newCompany.turnover" required />

                                            </div>
                                        </div>
                                        <div class="subscription-column">
                                            <div class="subscription-header"> <span class="text-colorful">Logo</span></div>
                                            <div class="subscription-body">
                                                <br />


                                                <input type="file" data-ng-model="newCompany.CompanyLogoPath" required />
                                                <img ng-src="{{newCompany.CompanyLogoPath}}" alt="No logo Image ">
                                            </div>
                                        </div>
                                    </div>
                                    <div class="subscription-footer">
                                        <div class="subscription-background">
                                            <div class="subscription-button-wrapper">
                                                <input type="submit" class="button-colorful" value="Register" name="CRegister" data-ng-click="add()" @*data-ng-disabled="!addCompany.$valid"*@ />
                                            </div>
                                        </div>
                                    </div>
                                </form>
                            </div>
                        </div><!-- end of .company-content-container -->

                    </div>

