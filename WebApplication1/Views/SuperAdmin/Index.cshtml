                                <tr>
                                    <th>#</th>
                                    <th>Name</th>
                                    <th>Email</th>
                                    <th></th>
                                </tr>
                                <tr>
                                    <td></td>
                                    <td>
                                        <input type="text" class="sidebar-search-input" placeholder="Search..." data-ng-model="search.userName" />
                                    </td>
                                    <td>
                                        <input type="text" class="sidebar-search-input" placeholder="Search..." data-ng-model="search.email" />
                                    </td>
                                    <td></td>
                                </tr>

                                <tr data-ng-repeat="user in users | filter:search">
                                    <td><strong data-ng-hide="user.editMode" ng-hide="true">{{ user.id }}</strong></td>
                                    <td>
                                        <p data-ng-hide="user.editMode">{{ user.userName }}</p>
                                        <p data-ng-show="user.editMode">
                                            <input type="text" data-ng-model="user.userName" />
                                        </p>
                                    </td>
                                    <td>
                                        <p data-ng-hide="user.editMode">{{ user.email }}</p>
                                        <input data-ng-show="user.editMode" type="text" data-ng-model="user.email" />
                                    </td>
                                    <td>
                                        <p data-ng-hide="user.editMode"><a data-ng-click="toggleEdit(user)" href="javascript:;">Edit</a> | <a data-ng-click="deluser(user)" href="javascript:;">Delete</a></p>
                                        <p data-ng-show="user.editMode"><a data-ng-click="save(user)" href="javascript:;">Save</a> | <a data-ng-click="toggleEdit(user)" href="javascript:;">Cancel</a></p>
                                    </td>
                                </tr>
                            </table>
                            <hr />
