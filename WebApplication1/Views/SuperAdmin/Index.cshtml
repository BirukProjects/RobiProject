        <div class="zone-company-content zone clearfix">

            <div id="company-inner-container" class="company-inner-container container-24">

                <div id="company-tabs-page" class="company-tabs-content">




                        <div data-ng-controller="CategoryController">
                            <div class="company-sidebar-container container-8">
                                <div class="products-list block">
                                    <div class="block-title">
                                        <h3>Catgeory List</h3>
                                    </div>
                                    <ul class="list-root">
                                        <li data-ng-repeat="category in categorys" ng-init="visible=true" ng-click="visible=!visible">
                                            <a href="">{{category.category1}}</a>
                                            <ul ng-show="visible">

                                                <li data-ng-repeat="categoryA in category.categoryAreas " ng-click="visible=visible"><a href="#"><div>{{categoryA.categoryName}}</div></a></li>
                                            </ul>
                                        </li>
                                    </ul>


                                </div>
                            </div>
                            <div class="company-content-container container-16">
                                <br />
                                <strong class="error">{{ error }}</strong>
                                <div class="our-directory block">
                                    <p data-ng-hide="addModeCat"><a data-ng-click="toggleAddCat()" href="javascript:;" class="button-2-colorful">Create New category</a></p>
                                    <form name="addCategory" data-ng-show="addModeCat" class="comment-message-form">

                                        <div class="comment-message">
                                            <div class="comment-message-title">
                                                <span class="text-colorful">Add New Category</span>
                                            </div>


                                            <label>Category Name:</label><input type="text" class="text-input-grey" data-ng-model="newCategory.category1" required />
                                            <label>Category Description:</label><input type="text" class="text-input-grey" data-ng-model="newCategory.Subcategory" required />
                                            <br />
                                            <span class="error" data-ng-show="addCategory.$error.Subcategory">Invalid Data!</span>
                                            <br />
                                            <input type="submit" value="AddCat" data-ng-click="addCat()" data-ng-disabled="!addCategory.$valid" class="button-2-colorful" />
                                            <input type="button" value="Cancel" data-ng-click="toggleAddCat()" class="button-2-colorful" />
                                            <br />
                                            <br />
                                        </div>
                                    </form>
                                </div>
                                <table class="table table-bordered table-hover table-responsive" >
                                    <tr>
                                        <th>#</th>
                                        <th>Category</th>
                                        <th>Description</th>
                                        <th></th>
                                    </tr>
                                    <tr>
                                        <td></td>
                                        <td>
                                            <input type="text" class="sidebar-search-input" placeholder="Search..." data-ng-model="search.category1" />
                                        </td>
                                        <td>
                                            <input type="text" class="sidebar-search-input" placeholder="Search..." data-ng-model="search.subcategory" />
                                        </td>
                                        <td></td>
                                    </tr>

                                    <tr data-ng-repeat="category in categorys | filter:search">
                                        <td><strong data-ng-hide="category.editMode" ng-hide="true">{{ category.CategoryID }}</strong></td>
                                        <td>
                                            <p data-ng-hide="category.editModeCat">{{ category.category1 }}</p>
                                            <p data-ng-show="category.editModeCat">
                                                <input type="text" data-ng-model="category.category1" />
                                            </p>
                                        </td>
                                        <td>
                                            <p data-ng-hide="category.editModeCat">{{ category.subcategory }}</p>
                                            <input data-ng-show="category.editModeCat" type="text" data-ng-model="category.subcategory" />
                                        </td>
                                        <td>
                                            <p data-ng-hide="category.editModeCat"><a data-ng-click="toggleEditCat(category)" href="javascript:;">Edit</a> | <a data-ng-click="delcategory(category)" href="javascript:;">Delete</a></p>
                                            <p data-ng-show="category.editModeCat"><a data-ng-click="saveCat(category)" href="javascript:;">Save</a> | <a data-ng-click="toggleEditCat(category)" href="javascript:;">Cancel</a></p>
                                        </td>
                                    </tr>
                                </table>
                                <hr />
                                <div data-ng-controller="CategoryAreaController">

                                    <br />
                                    <strong class="error">{{ error }}</strong>
                                    <div class="our-directory block">
                                        <p data-ng-hide="addMode"><a data-ng-click="toggleAdd()" href="javascript:;" class="button-2-colorful">Create New categoryArea</a></p>
                                        <form name="addCategoryArea" data-ng-show="addMode" class="comment-message-form">

                                            <div class="comment-message">
                                                <div class="comment-message-title">
                                                    <span class="text-colorful">Add New CategoryArea</span>
                                                </div>


                                                <label>Category Name </label><input type="text" class="text-input-grey" data-ng-model="newCategoryArea.categoryName" required />
                                                <br />
                                                <select data-ng-model="newCategoryArea.categoryID" class="select-input-grey" data-ng-options="c.categoryID as c.category1 for c in categorys">
                                                    <option value="">-- Select Main Category --</option>
                                                </select>
                                                <br />
                                                <label>Icon Path: </label><input type="text" class="text-input-grey" data-ng-model="newCategoryArea.iconPath" required />
                                                <label>Approved:</label><input type="checkbox" data-ng-model="newCategoryArea.approved" required />
                                                <br />
                                                <div class="notification-general">
                                                    <div class="notification-inner">
                                                        <span class="error"></span>
                                                        <a class="close" href="#"></a>
                                                    </div>
                                                </div>

                                                <br />
                                                <input type="submit" value="Add" data-ng-click="add()" data-ng-disabled="!addCategoryArea.$valid" class="button-2-colorful" />
                                                <input type="button" value="Cancel" data-ng-click="toggleAdd()" class="button-2-colorful" />
                                                <br />
                                                <br />
                                            </div>
                                        </form>
                                    </div>
                                    <table class="table table-bordered table-hover table-responsive" >
                                        <tr>
                                            <th>#</th>
                                            <th>CategoryArea</th>
                                            <th>CategoryName</th>
                                            <th>Icon</th>
                                            <th></th>
                                        </tr>
                                        <tr>
                                            <td></td>
                                            <td>
                                                <input type="text" class="sidebar-search-input" placeholder="Search..." data-ng-model="search.categoryArea1" />
                                            </td>
                                            <td>
                                                <input type="text" class="sidebar-search-input" placeholder="Search..." data-ng-model="search.subcategoryArea" />
                                            </td>
                                            <td></td>
                                        </tr>

                                        <tr data-ng-repeat="categoryArea in categoryAreas | filter:search">
                                            <td><strong data-ng-hide="categoryArea.editMode" ng-hide="true">{{ categoryArea.categoryAreaId }}</strong></td>
                                            <td>
                                                <p data-ng-hide="categoryArea.editMode">{{ categoryArea.categoryName }}</p>
                                                <p data-ng-show="categoryArea.editMode">
                                                    <input type="text" data-ng-model="categoryArea.categoryName" />
                                                </p>
                                            </td>
                                            <td>
                                                <p data-ng-hide="categoryArea.editMode">{{ categoryArea.approved }}</p>
                                                <input data-ng-show="categoryArea.editMode" type="text" data-ng-model="categoryArea.approved" />
                                            </td>
                                            <td>
                                                <p data-ng-hide="categoryArea.editMode">{{ categoryArea.IconPath }}</p>
                                                <input data-ng-show="categoryArea.editMode" type="text" data-ng-model="categoryArea.IconPath" />
                                            </td>
                                            <td>
                                                <p data-ng-hide="categoryArea.editMode"><a data-ng-click="toggleEdit(categoryArea)" href="javascript:;">Edit</a> | <a data-ng-click="delcategoryArea(categoryArea)" href="javascript:;">Delete</a></p>
                                                <p data-ng-show="categoryArea.editMode"><a data-ng-click="save(categoryArea)" href="javascript:;">Save</a> | <a data-ng-click="toggleEdit(categoryArea)" href="javascript:;">Cancel</a></p>
                                            </td>
                                        </tr>
                                    </table>
                                    <hr />
                                </div>
                            </div>


                    </div><!-- end of .company-content-container -->

